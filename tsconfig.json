{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": false,
      "target": "ES6",
      "noImplicitAny": true,
      "moduleResolution":"Node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "resolveJsonModule": true,
      "paths": {
          "*":[
              "node_modules/",
              "src/types/*"
          ]
      }
    },
    "include": [
        "src/**/*"
    ]
}